PEXER01{-----------------------------------------------------pexer}
#include <stdio.h>

int main()

{
float SP;
float OP;
const float DC = .85;

 printf("Insert Selling Price here: ");
 scanf("%f", &SP);
 
   OP = SP / DC;
   
  printf("This is the Original Price: %.2f", OP);

return 0;
}



PEXER02{-----------------------------------------------------pexer}
#include <stdio.h>

int main()
{
 float Yard;
 float Ft;
   float Inch;
   float CM;
	 float M;
	 
	 printf("Enter Length of Yard here: ");
	 scanf("%f", &Yard);
	   
	  Ft = Yard * 1;
	   Inch = Ft * 12;
	    CM = Inch * 2.54;
		 M = CM / 100;
	
    printf("This is the Feet: %.2f\n", Ft);
	printf("This is the Inch: %.2f\n", Inch);
   printf("This is the Centimeters: %.2f\n", CM);
  printf("This is the Meters: %.2f", M);

return 0;
}



PEXER03{-----------------------------------------------------pexer}
#include <stdio.h>

int main(){
    
    float iPrice;
	 float amntTent;
	  float iPrice2;
	   float amntTent2;
	   float change;
	    int change2, p20, p10, p5, p1, c25, c10, c05;
	   
	  printf("Item Price: ");
	    scanf("%f", &iPrice);
	  printf("Amount Tentered: ");
	    scanf("%f", &amntTent);
		
		 iPrice2 = iPrice / 100;
		  amntTent2 = amntTent / 100;
		   change = amntTent2 - iPrice2; 
		   
	  printf("\n\nItem Price: %.2f", iPrice2);
	   printf("\nAmount Tentered: %.2f", amntTent2);
	    printf("\n\n\nChange: %.2f Denomination", change);
	      
		  change2 = change * 100;
		   p20 = change2 / 2000;
		    p10 = change2 / 1000;
			 p5 = change2 / 500;
			  p1 = change2 / 100 % 1;
			 c25 = change /250;
			c10 = change / 100;
		   c05 = change / 50;
		   
	  printf("\n\n20 Pesos: %d", p20);
	   printf("\n10 Pesos: %d", p10);
	    printf("\n 5 Pesos: %d", p5);
		 printf("\n 1 Pesos: %d", p1);
		printf("\n.25 Centavo: %d", c25);
	   printf("\n.10 Centavo: %d", c10);
	  printf("\n.05 Centavo: %d", c05);

return 0;	 	 	  
}


