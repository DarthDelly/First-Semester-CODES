#include <stdio.h>

int main(){
   float disM;
   float fare;
    printf("Enter Distance Meters: ");
	 scanf("%f", &disM);
	 
   if(300>=disM)
   {
    printf("\nAmount Money to Pay: P 20.00");
	  
   }
   else
   {
    if(550>=disM){
	 printf("\nAmount to Pay: P 22.00");}
	else{
	 if(1000>=disM){
	  printf("\nAmount to Pay: P 26.00");}
	 else{
	  printf("\nAmount to Pay: P 42.00");
	  }
	 }
	}
   

return 0;
}



MEXER02{-----------------------------------------------------mexer}
#include <stdio.h>

int main(){
   float WSt, RP, WS, WC, WGP;
   
    printf("Enter Weekly Sales: ");
	 scanf("%f", &WSt);
	 
	  if(WSt>=5000){
	   RP = 1000;
	   WS = WSt;
	   WC = WSt * 0.097;
	   WGP = RP + WC;
	  }
	  else{
	   RP = 1000;
	   WS = WSt;
	   WC = 250;
	   WGP = RP + WC;
	  }
	   
	   printf("\n\nRegular Pay: %.2f", RP);
	    printf("\nWeekly Sales: %.2f", WS);
		 printf("\nWeekly Commission: %.2f", WC);
		  printf("\nWeekly Gross Pay: %.2f", WGP);
	   
return 0;
}



PEXER03{-----------------------------------------------------mexer}
#include <stdio.h>

int main(){
   int KwtH, Con;
    float FBill;
    
	printf("Kilowatt-Hour: ");
	 scanf("%d", &KwtH);
	 
	 Con = 50 * KwtH;
	 FBill = Con * 0.085;
	  
	  printf("\n\nConsumption: %d", Con);
	  printf("\nFinal Electric Bill: %.2f\n\n", FBill);
	  
return 0;
}



PEXER04{-----------------------------------------------------mexer}
#include <stdio.h>

int main(){
  int n1, n2, n3, large;
  
   printf("Enter N1: ");
    scanf("%d", &n1);
	
	 printf("Enter N2: ");
	  scanf("%d", &n2);
	  
	   printf("Enter N3: ");
	    scanf("%d", &n3);
		
     if(n1 >= n2){
	  if(n1 >= n3){
	   large = n1;
	  }
	  else{
	   large = n2;
	  }
	 }
	 else{
	  if(n2 >= n3){
	   large = n2;
	  }
	  else{
	   large = n3;
	  }
	 }
	   printf("\n\nThe largest integer: %d", large);
	 
return 0;
}



PEXER05{-----------------------------------------------------mexer}
#include <stdio.h>

int main(){
  float GS, tax;
  
   printf("Enter Grossly Salary: ");
    scanf("%f", &GS);
	
	if(GS <= 50000){
	 tax = 0.10 * GS;
	}
	else{
	
	  if(GS <= 100000){
	   tax = 5000 + (0.15 * GS);
	  }
	  else{
	    if(GS <= 200000){
		 tax = 15000 + (0.2 * GS);
		}
		else{
		 tax = 0.25 * GS;
		}
	  }
	}
	
	 printf("Tax to be Paid: %.2f", tax);
	 
return 0;
}



PEXER06{-----------------------------------------------------mexer}
#include<stdio.h>

int main(){
  int t, d, des;
   float A, ASR, AFR, ER;
  
   printf("INPUT: American Region (1), Asian Region (2), African Region (3), European Region (4)\nEnter Destination: ");
    scanf("%d", &des);
	
	 printf("\n\n(INPUT 1 for DAY and 2 for NIGHT)\nEnter Time: ");
	  scanf("%d", &t);
	  
	   printf("\n\nINPUT (Minutes Only)\nEnter Duration of the call: ");
	    scanf("%d", &d);
		
   if(t < 2){
	   if(des == 1){
	   A = (d / 3) * 50; 
	   printf("\nTotal Charges: %.2f", A);} 
	   
	   else if(des == 2){
	   ASR = d / 2 * 30; 
	   printf("\nTotal Charges: %.2f", ASR);}
	  
	   else if(des == 3){
	   AFR = d / 3 * 40; 
	   printf("\nTotal Charges: %.2f", AFR);}
	  
	   else if(des == 4){
	   ER = d / 2 * 35; 
	   printf("\nTotal Charges: %.2f", ER);
	 } 
	} 
	else {
       if(des == 1){
	   A = d / 3 * 45;
	   printf("\nTotal Charges: %.2f", A); 
	 } else if(des == 2){
	   ASR = d / 2 * 27; 
	   printf("\nTotal Charges: %.2f", ASR);
	 } else if(des == 3){
	   AFR = d / 3 * 36; 
	   printf("\nTotal Charges: %.2f", AFR);
	 } else if(des == 4){
	   ER = d / 2 * 30; 
	   printf("\nTotal Charges: %.2f", ER);
	 } 
	}

return 0;
}




SEMI-FINALS{-----------------------------------------------------semi-finals}
#include <stdio.h>

int main(){
  int crs, sub, year, ts, TentAmnt, stntID, ta, sub1;
     char first[20];
	 char last[20];
	 char gender[20];
	   char *ones[] = {"", "One", "Two", "Three", "Four", "Five", "Six", "Seven", "Eight", "Nine", "Ten", "Eleven", "Twelve", "Thirteen", "Fourteen", "Fifteen", "Sixteen", "Seventeen", "Eighteen","Nineteen"};
	   char *tens[] = {"", "", "Twenty", "Thirty", "Forty", "Fifty", "Sixty", "Seventy", "Eighty", "Ninety"};
	
	 printf("Enter ID Number: ");
	  scanf("%d", &stntID);
	  
	   printf("Enter First Name: ");
	    scanf("%s", &first);
		
		 printf("Enter Last Name: ");
		  scanf("%s", &last);
		 
		 printf("[Male, Female, Gay, Lesbian])\nGender: ");
		  scanf("%s", &gender);
		    printf("    1-BSIT\n    2-BSHM\n    3-BSED\n    4-COMPE\nCourse: ");
	      	 scanf("%d", &crs);
			   
		printf("Year: ");
		 scanf("%d", &year);
		 
	   printf("Number of Subjects Enrolled: ");
	    scanf("%d", &sub);
		
	  printf("Amount Tentered: ");
	   scanf("%d", &TentAmnt);
	   
	  sub = sub * 3;
	  sub1 = sub * 450;
	  
	  
	if(crs < 2){
	  ta = 545 + 1000 + 3000;
	    ts = sub1 + ta;
	}
	else if(crs < 3){
	  ta = 550 + 1000 + 3500;
		ts = sub1 + ta;
	}
	else if(crs < 4){
	  ta = 555 + 1000 + 1500;
		ts = sub1 + ta;
	}
	else{
	  ta = 560 + 1000 + 3000;
	 	 ts = sub1 + ta;
	}
	   
	   
if(ts < TentAmnt){	      
	if(crs==1) {
	 printf("\n\n\n\nUniversity of Cebu - Main Campus");
	  printf("\n\nOFFICIAL RECEIPT");
	   printf("\n\nID Number		: %d", stntID);
	    printf("\nName			: %s, %s", first, last);
	     printf("\nGender			: %s", gender);
	    printf("\nCourse			: BSIT-%d", crs, year);
	   printf("\nTotal Units		: %d", sub);
	  printf("\nTotal Assessment	: %d", ts);
	  } 
	 else if(crs==2){
	  printf("\n\n\n\nUniversity of Cebu - Main Campus");
	  printf("\n\nOFFICIAL RECEIPT");
	   printf("\n\nID Number		: %d", stntID);
	    printf("\nName			: %s, %s", first, last);
	     printf("\nGender			: %s", gender);
	    printf("\nCourse			: BSHM-%d", year);
	   printf("\nTotal Units		: %d", sub);
	  printf("\nTotal Assessment	: %d", ts);
	}
	 else if(crs==3){
	printf("\n\n\n\nUniversity of Cebu - Main Campus");
	  printf("\n\nOFFICIAL RECEIPT");
	   printf("\n\nID Number		: %d", stntID);
	    printf("\nName			: %s, %s", first, last);
	     printf("\nGender			: %s", gender);
	    printf("\nCourse			: BSED-%d", year);
	   printf("\nTotal Units		: %d", sub);
	  printf("\nTotal Assessment	: %d", ts);
	}
	  else if(crs==4){
	 printf("\n\n\n\nUniversity of Cebu - Main Campus");
	  printf("\n\nOFFICIAL RECEIPT");
	   printf("\n\nID Number		: %d", stntID);
	    printf("\nName			: %s, %s", first, last);
	     printf("\nGender			: %s", gender);
	    printf("\nCourse			: COMPE-%d", year);
	   printf("\nTotal Units		: %d", sub);
	  printf("\nTotal Assessment	: %d", ts);
	}
	 
  
	

	printf("\nTurn Numbers into Words :");
	 
	
	if(ts >= 1000){
	 printf(" %s Thousand", ones [ts / 1000]);
	  ts %= 1000;
	}
	if(ts >= 100){
	 printf(" %s Hundred", ones [ts / 100]);
	  ts %= 100;
	}
	if(ts >= 10){
	 printf(" %s", tens [ts / 10]);
	  ts %= 10;
	}
	if(ts <= 10){
	 printf(" %s\n\n", ones [ts / 1]);
	  ts %= 1;
	}
    }
	
else{
  printf("\n\n[Error: Transaction Cancelled-Insufficient Money]\n\n");
}

	
	
return 0;	 	 
}




FEXER01{-----------------------------------------------------fexer}
#include <stdio.h>

int main(){
   int integers[10], i, largest, smallest;
   
    printf("Input Intergers: \n");
	
   for (i=0; i<10; i++){
     scanf("%d", &integers[i]);
   }
   
   largest = smallest = integers[0];
   
   for (i=1; i<10; i++){
    if (integers[i] > largest){
     largest = integers[i];
    }
    if (integers[i] < smallest){
	 smallest = integers[i];
	} 
   }
    
	printf("\nLargest Number: %d\n", largest);
	printf("Smallest Number: %d", smallest);

return 0;
}



FEXER02{-----------------------------------------------------fexer}
#include <stdio.h>

int main(){
 int A=0, B=0, i, score;
 
  while (1){
   printf("Input (Team A)=1, (Team B)=2, (Stop)=0: ");
    scanf("%d", &score);
	
	if (score == 1){
	 printf("Team A Goal!\n\n");
	  A++;
	}
	else if (score == 2){
	 printf("Team B Goal!\n\n");
	  B++;
	}
	else if (score == 0){
	 break;
	}
	else {
	 printf("Invalid Value Inputted, Please Try Again\n\n");
	}
  }
  
   
    printf("Overall Score: \n\n");
	 printf("Team A: %d\n", A);
     printf("Team B: %d\n", B);
	 
  if (A>B) {
   printf("Team A Win!");
  }
  if (B>A) {
   printf("Team B Win!");
  }
  else {
   printf("Its a Tie");
   
return 0;  
}
  
}



FEXER03{-----------------------------------------------------fexer}
#include <stdio.h>

int main(){
 long p = 2, i, total = 0, num;
 printf("Day 1: P1\n");
  for (i=2; i<=30; i++){
   
    printf("Day %d: P%ld\n", i, p);
	
  total += p;
   
   p *= 2;
  }
  
   printf("\nTotal Payment in 30 days: %ld", total);
   
return 0;
}




FEXER04{-----------------------------------------------------fexer}
#include <stdio.h>

int main(){
 int digit, i, total = 0;
 
 printf("\n\nInput [Kill; 1], [Slained: 2], [Slained Roshan: 3], [Currier Slained: 4], [STOP Program: Negative#]: ");
 
  while (1){
    scanf("%d", &digit);
	
	if (digit == 1){
	 printf("\nYou Slain the Opponent Hero\n");
	  total++;
	}
	else if (digit == 2){
	 printf("\nYou're Slain by Neutral Creep, Noob!");
	}
	else if (digit == 3){
	 printf("\nYou're Slain by Roshan");
	}
	else if (digit == 4){
	 printf("\nYou're Currier has been Slain");
	}
	else if (digit <= 0){
	 break;
	}
	else {
	 printf("Invalid Input");
	}

  if (total == 1){
   printf("First Kill");
  }
  else if (total == 3){
   printf("Killing Spree");
  }
  else if (total == 4){
   printf("Dominating");
  }
  else if (total == 5){
   printf("Megakill");
  }
  else if (total == 6){
   printf("Unstoppable");
  }
  else if (total == 7){
   printf("Wicked Sick");
  }
  else if (total == 8){
   printf("Monster Kill");
  }
  else if (total == 9){
   printf("Godlike");
  }
  else if (total == 10){
   printf("Beyond Godlike");
  }
  
  printf("\n\n");
  }
  
  
  
return 0;
}




FEXER05{-----------------------------------------------------fexer}
#include <stdio.h>

void conMeters(float num){
 float meters, feet, inch, centi;
  meters = num * 0.9144;
  feet = meters * 3.28084;
  inch = feet * 12;
  centi = inch * 2.54;
 
 printf("\nConvert Yards to Meters: %.2f", meters);
 printf("\nConvert Meters to Feet: %.2f", feet);
 printf("\nConvert Feet to Inches: %.2f", inch);
 printf("\nConvert Inches to Centimeters: %.2f", centi);
}

int main(){
 float number;
   printf("Input a number in Length: ");
    scanf("%f", &number);
	 
 conMeters(number);
 
return 0;
}




SEATWORK{-----------------------------------------------------seatwork}
#include <stdio.h>
    
    int main(){
      int year;
      float amnt, intrst;
       
     amnt= 1;
     year= 2;
     
       for (;(amnt>=0 && year>=1);){
         printf("Input Amount: ");
    	  scanf("%f", &amnt);
    	 printf("Enter Year: ");
    	  scanf("%d", &year);
    	  
    	   intrst= amnt * year * .10;
       }
       while (;(amnt<=0 && year<=1);){
        printf("Insufficient Amount");
       }
       
       printf("Amount Total: %.2f", intrst);
    
    return 0;
    }
    

